<!--pages/my/my.wxml-->
<!-- 个人信息 -->
<view class='my_info'>
  <image class='my_info_hand' src='{{userinfo.avatarUrl}}' mode='aspectFill'></image>
  <view class='my_info_name'>{{userinfo.nickName}}</view>
  <view class='my_info_change' bindtap='to_myinfo'>编辑个人资料</view>
</view>
<!-- 选项 -->

