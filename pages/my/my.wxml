    <image  mode='aspectFill' class='some_to' src='../../images/forme_to.png'></image>
  </view>
  <view class='some'  bindtap='to_child'  id="1" >
    <image   mode='aspectFit' class='some_img' src='../../images/myinact.png'></image>
    <view class='some_title'>我发布的活动</view>
    <image  mode='aspectFit' class='some_to' src='../../images/forme_to.png'></image>    
  </view>
    <view class='some'  bindtap='to_news'   >
